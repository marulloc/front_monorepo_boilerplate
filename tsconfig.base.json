{
    "compilerOptions": {
        "module": "ES6", //
        "target": "ES6", //
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "allowJs": true,
        "jsx": "preserve",
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "incremental": true,

        "sourceMap": true,
        "noImplicitAny": false
    }

    //Refrences For Libs ...추후에
    // "references": []
}
